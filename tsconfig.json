{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        "moduleResolution": "node",
        "sourceMap": true,
        "strict": true,
        "noImplicitAny": true,
        "outDir": "./dist",
        "forceConsistentCasingInFileNames": true,
        "strictFunctionTypes": true,
        "noImplicitReturns": true
    },
    "lib": ["es2015"]
}
